def demonstrate_data_types():
    # Integer
    my_int = 10
    # Floating-Point
    my_float = 20.5
    # String
    my_string = "Hello, World!"
    # Boolean
    my_bool = True
    # List
    my_list = [1, 2, 3, 4, 5]
    # Tuple
    my_tuple = (10, 20, 30)
    # Dictionary
    my_dict = {"name": "Alice", "age": 30}
    # Set
    my_set = {1, 2, 3, 4, 5}
    
    # Displaying each type with test output
    print("Integer:", my_int)                  # Test Case 1: Integer
    print("Float:", my_float)                  # Test Case 2: Floating-Point
    print("String:", my_string)                # Test Case 3: String
    print("Boolean:", my_bool)                 # Additional: Boolean
    print("List:", my_list)                    # Additional: List
    print("Tuple:", my_tuple)                  # Additional: Tuple
    print("Dictionary:", my_dict)              # Additional: Dictionary
    print("Set:", my_set)                      # Additional: Set

# Run the function
demonstrate_data_types()
